{
  "name": "alp",
  "version": "6.0.0",
  "description": "alp framework for node 5 and for browser",
  "keywords": [
    "springbokjs",
    "framework",
    "ibex",
    "node5"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "alpjs/alp",
  "homepage": "https://github.com/alpjs/alp#readme",
  "bugs": {
    "url": "https://github.com/alpjs/alp/issues"
  },
  "main": "index.js",
  "webpack:main": "./lib-webpack/browser.js",
  "webpack:main-dev": "./lib-webpack-dev/browser.js",
  "webpack:main-modern-browsers": "./lib-webpack-modern-browsers/browser.js",
  "webpack:main-modern-browsers-dev": "./lib-webpack-modern-browsers-dev/browser.js",
  "scripts": {
    "build": "pob-build",
    "build:dev": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "eslint --fix -c .eslintrc.yml src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit || echo",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "version": "pob-version",
    "watch": "pob-watch",
    "watch:dev": "pob-watch"
  },
  "dependencies": {
    "alp-browser": "^0.12.1",
    "alp-node": "^0.12.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-modern-browsers": "^5.1.0",
    "babel-preset-modern-browsers-stage-1": "^1.0.0",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-config-pob": "^6.1.6",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "pob-babel": "^7.1.2",
    "pob-release": "^2.1.0"
  }
}
